<?xml version="1.0"?>
<extension key="biz.jmaconsulting.churlrewrite" type="module">
  <file>churlrewrite</file>
  <name>CanadaHelps URL Re-write Extension</name>
  <description>Extension for CanadaHelps to re-write urls to remove civicrm/ and replace with dms/</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Seamus Lee</author>
    <email>seamus.lee@jmaconsulting.biz</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/CanadaHelps/biz.jmaconsulting.churlrewrite</url>
    <url desc="Documentation">https://github.com/CanadaHelps/biz.jmaconsulting.churlrewrite</url>
    <url desc="Support">https://github.com/CanadaHelps/biz.jmaconsulting.churlrewrite</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2020-09-22</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>This is a module that depends on the Drupal 7 module Sub Path Alias https://www.drupal.org/project/subpathauto to work correctly.</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Churlrewrite</namespace>
  </civix>
</extension>
